//DB name: db

CREATE TABLE usrInfo(
ID TEXT NOT NULL PRIMARY KEY,
PW TEXT NOT NULL, 
Enable TINYINT default 0,
Gender INT);


//key is ID ,,, Enable is TINYINT with 1 for enable 0 for disable(default is 0) ,, Gender is 0 or 1(0==male)


CREATE TABLE usrE (
ID TEXT NOT NULL FOREIGN KEY,
Email TEXT NOT NULL PRIMARY KEY,
);
